<?xml version="1.0" encoding="utf-8"?><!--车辆核查*******车辆中标信息-->
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/rl_detail_car_bid_info"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="@dimen/dp_8"
    android:layout_marginTop="8dp"
    android:background="@color/white"
    android:padding="10dp"
    android:visibility="gone">

    <ImageView
        android:id="@+id/iv_control_square"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/blue_dot_shape" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_toEndOf="@id/iv_control_square"
        android:text="中标车辆信息" />

    <View
        android:id="@+id/divider_controlLine"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_below="@id/iv_control_square"
        android:background="@drawable/divider_with_padding" />
    <!--展示中标车辆信息-->
    <TextView
        android:id="@+id/tv_detail_car_bid_carType"
        android:layout_width="88dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/divider_controlLine"
        android:layout_marginTop="10dp"
        android:text="@string/car_type"
        android:textSize="@dimen/sp_14" />

    <TextView
        android:id="@+id/tv_detail_car_bid_carTypeIn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/divider_controlLine"
        android:layout_marginTop="10dp"
        android:layout_toEndOf="@+id/tv_detail_car_bid_carType"
        android:text=""
        android:textColor="#000"
        android:textSize="@dimen/sp_16" />

    <TextView
        android:id="@+id/tv_detail_car_bid_policeName"
        android:layout_width="88dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_detail_car_bid_carType"
        android:layout_marginTop="10dp"
        android:text="@string/policeName"
        android:textSize="@dimen/sp_14" />

    <TextView
        android:id="@+id/tv_detail_car_bid_policeNameIn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_detail_car_bid_carType"
        android:layout_marginTop="10dp"
        android:layout_toEndOf="@+id/tv_detail_car_bid_policeName"
        android:text=""
        android:textColor="#000"
        android:textSize="@dimen/sp_16" />

    <TextView
        android:visibility="gone"
        android:id="@+id/tv_detail_car_bid_taskName"
        android:layout_width="88dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/tv_detail_car_bid_taskNameIn"
        android:layout_alignBottom="@+id/tv_detail_car_bid_taskNameIn"
        android:layout_alignParentStart="true"
        android:text="@string/person_taskName"
        android:textSize="@dimen/sp_14" />

    <TextView
        android:visibility="gone"
        android:id="@+id/tv_detail_car_bid_taskNameIn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_detail_car_bid_policeName"
        android:layout_marginTop="10dp"
        android:layout_toEndOf="@+id/tv_detail_car_bid_taskName"
        android:text=""
        android:textColor="#000"
        android:textSize="@dimen/sp_16" />

    <TextView
        android:id="@+id/tv_detail_car_bid_taskDesc"
        android:layout_width="88dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_detail_car_bid_policeName"
        android:layout_marginTop="10dp"
        android:text="@string/control_remark"
        android:textSize="@dimen/sp_14" />

    <TextView
        android:id="@+id/tv_detail_car_bid_taskDescIn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/tv_detail_car_bid_taskDesc"
        android:layout_alignBottom="@+id/tv_detail_car_bid_taskDesc"
        android:layout_toEndOf="@+id/tv_detail_car_bid_taskDesc"
        android:text=""
        android:textColor="#000"
        android:textSize="@dimen/sp_16" />

    <TextView
        android:id="@+id/tv_detail_car_bid_linkPerson"
        android:layout_width="88dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_detail_car_bid_taskDesc"
        android:layout_marginTop="10dp"
        android:text="@string/control_linkPerson"
        android:textSize="@dimen/sp_14" />

    <TextView
        android:id="@+id/tv_detail_car_bid_linkPersonIn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_detail_car_bid_taskDesc"
        android:layout_marginTop="10dp"
        android:layout_toEndOf="@+id/tv_detail_car_bid_linkPerson"
        android:text=""
        android:textColor="#000"
        android:textSize="@dimen/sp_16" />

    <TextView
        android:id="@+id/tv_detail_car_bid_linkPhone"
        android:layout_width="88dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_detail_car_bid_linkPerson"
        android:layout_marginTop="10dp"
        android:text="@string/control_linkPhone"
        android:textSize="@dimen/sp_14" />

    <TextView
        android:id="@+id/tvdetail_car_bid_linkPhoneIn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_detail_car_bid_linkPerson"
        android:layout_marginTop="10dp"
        android:layout_toEndOf="@+id/tv_detail_car_bid_linkPhone"
        android:text=""
        android:textColor="#000"
        android:textSize="@dimen/sp_16" />


    <TextView
        android:visibility="gone"
        android:id="@+id/tv_detail_car_bid_checkedDesc"
        android:layout_width="88dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_detail_car_bid_linkPhone"
        android:layout_marginTop="10dp"
        android:text="@string/car_checked_description"
        android:textSize="@dimen/sp_14" />

    <TextView
        android:visibility="gone"
        android:id="@+id/tv_detail_car_bid_checkedDescIn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_detail_car_bid_linkPhone"
        android:layout_marginTop="10dp"
        android:layout_toEndOf="@+id/tv_detail_car_bid_checkedDesc"
        android:text="违停"
        android:textColor="#000"
        android:textSize="@dimen/sp_16" />
</RelativeLayout>
